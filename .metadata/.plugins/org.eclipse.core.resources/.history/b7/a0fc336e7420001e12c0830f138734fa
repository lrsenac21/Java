package funcoesMatematicas;

import java.util.Locale;

public class Main {

	public static void main(String[] args) {
		Locale.setDefault(Locale.US);
		
		double x = 3.0;
		double y = 4.0;
		double z=-5.0;
		double A, B, C;
		
		
		A = Math.sqrt(x);
		B = Math.sqrt(y);
		C = Math.sqrt(25.0);
		
		System.out.printf("A raiz quadrada de %.3f é = %.2f %n", x, A);
		System.out.printf("A raiz quadrada de %.2f é = %2f %n", y, B);

	}

}
