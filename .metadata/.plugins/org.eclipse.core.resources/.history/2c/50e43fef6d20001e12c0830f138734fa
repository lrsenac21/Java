	public static void main(String[] args) {		
	
		Locale.setDefault(Locale.US);
		
		
		
		Scanner scanner = new Scanner(System.in);
		char y;
		y = scanner.next().charAt(0);
		
		System.out.println("VocÃª digitou: " + y);
		
		scanner.close();
	}

}
